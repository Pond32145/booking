{
  "version": 2,
  "routes": [
    {
      "src": "/assets/(.*)",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/[^.]*",
      "dest": "/index.html"
    }
  ],
  "buildCommand": "bash vercel-build.sh",
  "outputDirectory": "dist"
}